{
	"name": "api",
	"version": "1.0.0",
	"main": "src/app.ts",
	"license": "MIT",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"start": "node dist/app.js",
		"dev": "ts-node-dev --respawn --transpile-only src/app.ts",
		"build": "tsc",
		"lint": "tsc --noEmit && eslint src/**/*.ts{,x} --cache --max-warnings=0",
		"lint:fix": "eslint src/**/*.ts{,x} --fix",
		"lint:check": "eslint --ignore-path .eslintignore --ext .js,.ts .",
		"prettier:check": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
		"lint:prettier": "yarn lint:check && yarn prettier:check",
		"start-degree": "ts-node-dev ./src/grpc/degree.ts",
		"start-processor": "ts-node-dev ./src/grpc/processor.ts",
		"start-grpc": "concurrently \"npm run start-degree\" \"npm run start-processor\"",
		"build-degree-proto": "proto-loader-gen-types --outDir=./src/grpc/degreeProto --grpcLib='@grpc/grpc-js' ./src/grpc/protos/degree.proto",
		"proto:gen": "proto-loader-gen-types --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=src/grpc/customer ./src/grpc/customer/customer.proto"
	},
	"lint-staged": {
		"src/**/*.ts": "yarn lint:prettier"
	},
	"dependencies": {
		"@grpc/grpc-js": "^1.10.10",
		"@grpc/proto-loader": "^0.7.13",
		"@types/authorizenet": "^1.0.2",
		"@types/bcrypt": "^5.0.0",
		"@types/cookie-parser": "^1.4.3",
		"@types/jsonwebtoken": "^9.0.2",
		"authorizenet": "^1.0.8",
		"axios": "^1.6.7",
		"babel-eslint": "^10.1.0",
		"bcrypt": "^5.1.0",
		"braintree": "^3.20.0",
		"card-validator": "^9.1.0",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"elastic-apm-node": "^4.7.3",
		"express": "^4.18.2",
		"express-fileupload": "^1.4.3",
		"express-rate-limit": "^7.1.5",
		"http-errors": "^2.0.0",
		"http-status": "^1.6.2",
		"ioredis": "^5.4.1",
		"jsonwebtoken": "^9.0.0",
		"lodash": "^4.17.21",
		"mongoose": "^7.1.1",
		"morgan": "^1.10.0",
		"socket.io": "^4.7.5",
		"winston": "^3.9.0",
		"winston-daily-rotate-file": "^4.7.1",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@types/braintree": "^3.3.11",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/express-fileupload": "^1.4.4",
		"@types/lodash": "^4.14.202",
		"@types/mongoose": "^5.11.97",
		"@types/node": "^22.5.2",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.5",
		"concurrently": "^8.2.2",
		"eslint": "^8.40.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.1",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"prettier": "^2.8.8",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.0.4"
	}
}
